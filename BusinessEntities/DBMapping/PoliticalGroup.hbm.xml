<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ro.stancescu.CDep.BusinessEntities" assembly="ro.stancescu.CDep.BusinessEntities">
  <class name="PoliticalGroupDBE" table="PoliticalGroup" dynamic-insert="true">
    <id name="Id">
      <generator class="native"/>
    </id>

    <property name="Name" column="Name"/>

    <set name="VotesCast" inverse="true" cascade="all">
      <key column="PoliticalGroup"/>
      <one-to-many class="VoteDetailDBE"/>
    </set>

  </class>
</hibernate-mapping>